{
  "expo": {
    "name": "greenTravel",
    "slug": "greenTravel",
    "ios": {
      "bundleIdentifier": "com.greentravel.radzima.expo.go",
      "supportsTablet": true,
      "sp`lash": {
        "image": "src/assets/bootsplash/bootsplash_logo.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "src/assets/bootsplash/bootsplash_logo_dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      }
    },
    "android": {
      "package": "app.radzima",
      "splash": {
        "image": "src/assets/bootsplash/bootsplash_logo@3x.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "src/assets/bootsplash/bootsplash_logo_dark@3x.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "src/assets/Icon-1024.png",
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "This will let to pinpoint your exact location."
        }
      ],
      [
        "expo-localization"
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.1.0",
          "RNMapboxMapsDownloadToken": ""
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e51feb59-6254-4001-bc5a-d7a5df40dbcb"
      }
    }
  }
}
